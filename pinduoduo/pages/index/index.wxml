<view class="container">
  <scroll-view class="cnt_hd_tab" scroll-x="true">
    <view wx:for="{{tabblock}}" class="tabBlock"  
    bindtap="tabClick">{{item.name}}</view>
  </scroll-view>

  <view class="block">

    <swiper class="swiper" indicator-dots="true" indicator-color="#ffffff" autoplay="true" interval="5000" duration="1000">
      <navigator class="block_hd" >
        <view wx:for="{{headerpic}}"> 
          <swiper-item>
            <image src="{{item.src}}" class="hdImage" />
          </swiper-item>
        </view>
      </navigator>
    </swiper>
  
